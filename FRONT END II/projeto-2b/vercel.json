{
    "version": 2,
    "builds": [
      {
        "src": "src/app/**/*.js",
        "use": "@vercel/node"
      },
      {
        "src": "src/components/**/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/app/$1"
      }
    ]
  }
  